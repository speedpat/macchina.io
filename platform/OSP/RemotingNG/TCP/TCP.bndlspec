<bundlespec>
	<manifest>
    	<name>Remoting NG TCP Transport</name>
		<symbolicName>com.appinf.osp.remotingng.tcp</symbolicName>
		<version>1.0.0</version>
		<vendor>Applied Informatics</vendor>
		<copyright>(c) 2017-2021, Applied Informatics Software Engineering GmbH</copyright>
		<activator>
			<class>Poco::OSP::RemotingNG::TCP::BundleActivator</class>
			<library>com.appinf.osp.remotingng.tcp</library>
		</activator>
		<lazyStart>false</lazyStart>
		<runLevel>000</runLevel>
		<dependency>
			<symbolicName>poco.net</symbolicName>
			<version>[1.0.0,2.0.0)</version>
		</dependency>
	</manifest>
	<code>
		${bin}/*.dll,
		${bin}/*.pdb,
		../../../${bin}/PocoRemotingNGTCP${64}.dll,
		../../../${bin}/PocoRemotingNGTCP${64}d.dll,
		../../../${bin}/PocoRemotingNGTCP${64}d.pdb,
		bin/${osName}/${osArch}/*.so,
		bin/${osName}/${osArch}/*.dylib,
		../../../lib/${osName}/${osArch}/libPocoRemotingNGTCP.so.*,
		../../../lib/${osName}/${osArch}/libPocoRemotingNGTCPd.so.*,
		../../../lib/${osName}/${osArch}/libPocoRemotingNGTCP.*.dylib,
		../../../lib/${osName}/${osArch}/libPocoRemotingNGTCPd.*.dylib
	</code>
	<files>
	</files>
</bundlespec>
